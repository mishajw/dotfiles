[user]
	email = mishajw@gmail.com
	name = Misha Wagner
	signingkey = 92271A1D
[commit]
	gpgsign = true
[push]
	default = current
[core]
	editor = vim
	excludesfile = /home/misha/.gitignore_global
[alias]
  a = add
  ap = add --patch
  b = branch
  c = "!f() { git commit --message \"$*\"; }; f"
  ca = "!f() { git commit --all --message \"$*\"; }; f"
  cam = commit --amend
  co = checkout
  d = diff
  ds = diff --staged
  f = fetch
  l = log --oneline --decorate --graph
  m = merge
  p = push
  pf = push --force
  pl = pull
  psup = push --set-upstream origin $(git_current_branch)
  rb = rebase
  rbi = rebase -i
  rbih = rebase -i HEAD~20
  s = status
  si = submodule init
  st = stash
  std = stash drop
  stp = stash pop
  su = submodule update

