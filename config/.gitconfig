[user]
	email = mishajw@gmail.com
	name = Misha Wagner
[push]
	default = current
	autoSetupRemote = true
[core]
	editor = vim
	excludesfile = ~/.gitignore_global
[filter "lfs"]
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
[diff]
	tool = vimdiff
[difftool]
	prompt = true
[rebase]
	autoStash = true
[include]
  path = ~/dotfiles/local/.gitconfig
[pull]
	rebase = true
